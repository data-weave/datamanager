{
    "name": "@data-weave/backend-firestore",
    "version": "0.4.3",
    "author": "",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*.js",
        "lib/**/*.d.ts"
    ],
    "scripts": {
        "build": "genversion --semi lib/version.js && cp -r ../../build/backend-firestore/lib/*.js lib/ && cp -r ../../build/types/backend-firestore/lib/*.d.ts lib/",
        "build:clean": "rimraf android/build && rimraf ios/build",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/data-weave/datamanager/tree/master/packages/backend-firestore"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "peerDependencies": {
        "@data-weave/datamanager": "0.4.3",
        "firebase": ">=11.0.0"
    }
}
